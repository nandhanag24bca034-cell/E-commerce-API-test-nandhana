				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response exists or not\", function()\r",
							" {\r",
							"    pm.expect(pm.response.text()).to.not.be.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Verify property exists\", function()\r",
							" {\r",
							"    pm.expect(pm.response.json()).to.have.property(\"ProductName\");\r",
							"});\r",
							"\r",
							"pm.test(\"Verify value of the property exists\", function()\r",
							" {\r",
							"    pm.expect(Number(pm.response.json().ProductCount)).to.be.above(0);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify value datatype\", function()\r",
							" {\r",
							"    pm.expect(pm.response.json().ProductCount).to.be.a(\"Number\");\r",
							"});\r",
							"\r",
							"pm.test(\"Product count value below range check\", function()\r",
							" {\r",
							"    pm.expect(Number(pm.response.json().ProductCount)).to.be.below(5000);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify response time\", function() \r",
							"{\r",
							"    pm.expect(pm.response.responseTime).to.be.below(10000);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify response time above\", function()\r",
							" {\r",
							"    pm.expect(pm.response.responseTime).to.be.above(0);\r",
							"});\r",
							"\r",
							"pm.test(\"Check multiple headers\", function()\r",
							" {\r",
							"    const headers = [\"content-length\", \"content-type\", \"date\"];\r",
							"    headers.forEach(h => {\r",
							"        pm.response.to.have.header(h);\r",
							"    });\r",
							"});\r",
							"\r",
							"pm.test(\"Test status code using if-else\", function()\r",
							" {\r",
							"    const data=pm.response.code;\r",
							"    if (data === 200) {\r",
							"        pm.response.to.have.status(\"OK\");\r",
							"    } else\r",
							"    {\r",
							"        pm.expect(data).to.not.eql(200);\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"Status code is not in oneOf function\", function()\r",
							" {\r",
							"    pm.expect(pm.response.code).to.not.oneOf([400,401,402,403,404]);\r",
							"});\r",
							"pm.collectionVariables.set(\"id\",pm.response.json().id);"
